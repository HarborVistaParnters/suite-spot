<section aria-labelledby="{{ module.heading | slugify }}" class="current-listings-list | section-spacing">
    <div class="content-wrapper">
        <h2 id="{{ module.heading | slugify }}">{{ module.heading }}</h2>
        <div class="fluid-grid" data-layout="thirds">
            {% for listing in collections.listing %}
                {% render 'partials/components/listing_preview.liquid',
                    listing: listing.data,
                    properties: collections.property
                %}
            {% endfor %}
        </div>
    </div>
</section>
